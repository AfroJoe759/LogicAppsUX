{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "OrderNo": {
      "type": "string"
    },
    "Status": {
      "type": "string"
    },
    "From": {
      "type": "string"
    },
    "CreateDate": {
      "type": "string"
    },
    "CreateTime": {
      "type": "string"
    },
    "ContactPerson": {
      "type": "object",
      "properties": {
        "First": {
          "type": "string"
        },
        "LastName": {
          "type": "string"
        },
        "Email": {
          "type": "string"
        },
        "Phone": {
          "type": "string"
        }
      },
      "required": ["First", "LastName", "Email", "Phone"]
    },
    "Items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Model": {
            "type": "string"
          },
          "Quantity": {
            "type": "integer"
          },
          "Price": {
            "type": "integer"
          },
          "Currency": {
            "type": "string"
          },
          "Discount": {
            "type": "number"
          }
        },
        "required": ["Model", "Quantity", "Price", "Currency", "Discount"]
      }
    }
  },
  "required": ["OrderNo", "Status", "From", "CreateDate", "CreateTime", "ContactPerson", "Items"]
}

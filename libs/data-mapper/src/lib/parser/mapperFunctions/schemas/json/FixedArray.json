{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
    {
      "Name": "abcd",
      "Role": ["a", "b", "c"],
      "Phone": [1, 2, 3],
      "FixedArray": [
        1,
        2,
        3,
        "a",
        "b",
        {
          "a": "a"
        }
      ]
    }
  ],
  "required": ["Name", "Role", "Phone", "FixedArray"],
  "properties": {
    "Name": {
      "$id": "#/properties/Name",
      "type": "string",
      "title": "The Name schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": ["abcd"]
    },
    "Role": {
      "$id": "#/properties/Role",
      "type": "array",
      "title": "The Role schema",
      "description": "An explanation about the purpose of this instance.",
      "default": [],
      "examples": [["a", "b"]],
      "additionalItems": true,
      "items": {
        "$id": "#/properties/Role/items",
        "anyOf": [
          {
            "$id": "#/properties/Role/items/anyOf/0",
            "type": "string",
            "title": "The first anyOf schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": ["a", "b"]
          }
        ]
      }
    },
    "Phone": {
      "$id": "#/properties/Phone",
      "type": "array",
      "title": "The Phone schema",
      "description": "An explanation about the purpose of this instance.",
      "default": [],
      "examples": [[1, 2]],
      "additionalItems": true,
      "items": {
        "$id": "#/properties/Phone/items",
        "anyOf": [
          {
            "$id": "#/properties/Phone/items/anyOf/0",
            "type": "integer",
            "title": "The first anyOf schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [1, 2]
          }
        ]
      }
    },
    "FixedArray": {
      "$id": "#/properties/FixedArray",
      "type": "array",
      "title": "The FixedArray schema",
      "description": "An explanation about the purpose of this instance.",
      "default": [],
      "examples": [[1, 2]],
      "additionalItems": true,
      "items": [
        {
          "$id": "#/properties/MixedArray/items/anyOf/0",
          "type": "integer",
          "title": "The first anyOf schema",
          "description": "An explanation about the purpose of this instance.",
          "default": 0,
          "examples": [1, 2]
        },
        {
          "$id": "#/properties/MixedArray/items/anyOf/1",
          "type": "string",
          "title": "The second anyOf schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": ["a", "b"]
        },
        {
          "$id": "#/properties/MixedArray/items/anyOf/2",
          "type": "object",
          "title": "The third anyOf schema",
          "description": "An explanation about the purpose of this instance.",
          "default": {},
          "examples": [
            {
              "a": "a"
            }
          ],
          "required": ["a"],
          "properties": {
            "a": {
              "$id": "#/properties/MixedArray/items/anyOf/2/properties/a",
              "type": "string",
              "title": "The a schema",
              "description": "An explanation about the purpose of this instance.",
              "default": "",
              "examples": ["a"]
            }
          },
          "additionalProperties": true
        }
      ]
    }
  },
  "additionalProperties": true
}
